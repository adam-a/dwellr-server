{
  "entryFile": "src/app.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": [
    "src/**/*Controller.ts"
  ],
  "spec": {
    "securityDefinitions": {
      "magic": {
        "type": "apiKey",
        "name": "x-did-token",
        "in": "header"
      }
    },
    "outputDirectory": "src/oapi",
    "specVersion": 3
  },
  "routes": {
    "routesDir": "src/oapi"
  }
}
